{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/browser/",
    "module": "esnext",
    "types": []
  },
  "include": [
    "src/core/**/*.ts",
    "src/libs/**/*.ts",
    "src/browser/**/*.ts"
  ],
  "exclude": [
    "src/main-process/**/*.ts",
    "src/browser/test.ts",
    "src/browser/**/*.spec.ts",
    "src/libs/**/*.spec.ts",
    "src/core/**/*.spec.ts",
    "src/browser/testing/"
  ]
}
