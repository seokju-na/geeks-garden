  "scripts": {
    "electron": "electron .",
    "serve:browser": "yarn serve:browser:app & yarn serve:browser:onboarding",
    "serve:browser:app": "ng build app --watch",
    "serve:browser:onboarding": "ng build onboarding --watch",
    "serve:main-process": "webpack --config webpack.electron.js --watch",
    "clean": "rimraf dist/",
    "build": "yarn clean && yarn build:browser && yarn build:main-process",
    "build:browser": "yarn build:browser:app && yarn build:browser:onboarding",
    "build:browser:app": "cross-env NODE_ENV=production ng build app --prod",
    "build:browser:onboarding": "cross-env NODE_ENV=production ng build onboarding --prod",
    "build:main-process": "cross-env NODE_ENV=production webpack --config webpack.electron.js",
    "lint": "yarn lint:browser && yarn lint:main-process",
    "lint:browser": "ng lint app",
    "lint:main-process": "tslint --project tsconfig.electron.json",
    "test": "yarn test:browser && yarn test:libs && yarn test:main-process",
    "test:browser": "ng test app",
    "test:libs": "mocha --opts src/libs/test.opts",
    "test:main-process": "electron-mocha --opts src/main-process/test.opts",
    "script:patch-angular": "node scripts/patch-angular.js",
    "script:rebuild": "electron-rebuild keytar",
    "postinstall": "yarn script:patch-angular && yarn script:rebuild"
  },
